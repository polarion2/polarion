[access]
	inheritFrom = All-Projects
[submit]
[access "refs/*"]
	owner = group user/polarion2
[access "refs/for/refs/*"]
	label-Code-Review = -2..+2 group user/polarion2
	label-Verified = -1..+1 group user/polarion2
	push = group user/polarion2
	read = group user/polarion2
	rebase = group user/polarion2
	removeReviewer = group user/polarion2
	submit = group user/polarion2
[access "refs/heads/*"]
	create = group user/polarion2
	push = +force group user/polarion2
	pushMerge = group user/polarion2
	read = group user/polarion2
[access "refs/tags/*"]
	push = group user/polarion2
	push = +force group user/polarion2
